{
  "info": {
    "name": "Flower Count API with Firebase",
    "description": "API to get flower count for images from Firebase and update results back",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Infer from Firebase",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/x-www-form-urlencoded",
            "type": "text"
          }
        ],
        "body": {
          "mode": "urlencoded",
          "urlencoded": [
            {
              "key": "plant_name",
              "value": "Bean Plant",
              "type": "text"
            },
            {
              "key": "firebase_uid",
              "value": "YOUR_FIREBASE_UID",
              "type": "text"
            },
            {
              "key": "secret_key",
              "value": "your_shared_secret_key",
              "type": "text"
            }
          ]
        },
        "url": {
          "raw": "http://localhost:8000/infer-from-firebase",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["infer-from-firebase"]
        }
      }
    }
  ]
}
